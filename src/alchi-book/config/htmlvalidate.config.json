{
  "extends": ["html-validate:recommended"],
  "rules": {
    "element-name": "off",
    "unrecognized-char-ref": "off",
    "no-inline-style": "off",
    "wcag/h37": "off",
    "void-style": "off",
    "prefer-tbody": "off",
    "doctype-style": "off",
    "svg-focusable": "off",
    "wcag/h67": "off",
    "element-permitted-content": "error"
  }
}
